<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item>
        <rotate  android:fromDegrees="135" android:toDegrees="0" android:pivotX="100%" android:pivotY="40%" >
            <shape android:shape="rectangle" >
                <stroke android:color="#B3B3B3" android:dashWidth="5dp" android:dashGap="2dp" android:width="2dp"/>
                <solid android:color="#C8C8C8" />
            </shape>
        </rotate>
    </item>
</layer-list>